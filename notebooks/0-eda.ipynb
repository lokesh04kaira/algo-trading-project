    "# download Apple stock data\n",
    "df = yf.download(\"AAPL\", start=\"2020-01-01\", end=\"2024-01-01\")\n",
    "print(df.head())\n",
    "\n",
    "# plot closing price\n",
    "df[\"Close\"].plot(title=\"AAPL Closing Price\", figsize=(12,6))\n",
    "plt.show()\n"
   ]
